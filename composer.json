{
    "name": "slam/phpstan-extensions",
    "type": "phpstan-extension",
    "description": "Slam extension of phpstan",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpstan/phpstan": "^0.11",
        "thecodingmachine/phpstan-strict-rules": "^0.11"
    },
    "require-dev": {
        "nette/di": "^3.0",
        "nette/neon": "^3.0",
        "phpstan/phpstan-php-parser": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^7.5",
        "roave/security-advisories": "dev-master",
        "slam/php-cs-fixer-extensions": "^1.18",
        "slam/php-debug-r": "^1.4"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "conf/slam-rules.neon"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SlamPhpStan\\": "lib/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestAsset/SymfonyFilesystemRule/mkdir.php",
            "tests/TestAsset/SymfonyProcessRule/shell_exec.php"
        ],
        "classmap": [
            "tests/"
        ]
    }
}
