{
    "name": "slam/phpstan-extensions",
    "description": "Slam extension of phpstan",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpstan/phpstan": "^0.10"
    },
    "require-dev": {
        "phpstan/phpstan-php-parser": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpunit/phpunit": "^7.1",
        "roave/security-advisories": "dev-master",
        "slam/php-cs-fixer-extensions": "^1.16",
        "slam/php-debug-r": "^1.2",
        "thecodingmachine/phpstan-strict-rules": "^0.10"
    },
    "autoload": {
        "psr-4": {
            "SlamPhpStan\\": "lib/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    }
}
